# Search Provider Configuration
# At least one provider must be configured
SERPER_API_KEY=
TAVILY_API_KEY=

# Models Configuration
# Path where ML models will be stored
# For Docker: mount a volume to this path
MODELS_PATH=/app/models

# LLM Configuration
OPENAI_API_KEY=
ANTHROPIC_API_KEY=

# Framework Validation
# Uncomment and set these to enable specific framework validation
# NEXTJS_VERSION=15
# SVELTEKIT_VERSION=5

# Build Configuration
# Default package manager for build validation
# Options: npm, yarn, pnpm, bun
DEFAULT_PACKAGE_MANAGER=pnpm

# Development Configuration
NODE_ENV=development
DEBUG=bolt:*
